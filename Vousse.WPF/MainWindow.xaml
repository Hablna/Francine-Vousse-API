<Window x:Class="Vousse.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vousse.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="text">
        <Button Content="choisir un fichier" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Import_Button"/>
        <TextBox x:Name="FilePathTextBox" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="300" Margin="0,0,0,50" IsReadOnly="True"/>
    </Grid>
</Window>
